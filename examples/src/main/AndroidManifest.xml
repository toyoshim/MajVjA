<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.twintail.majvj.examples" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="MajVjExamples" >
        <activity
            android:name=".Example1"
            android:label="MajVjExample1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Example2"
            android:label="MajVjExample2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Example3"
            android:configChanges="orientation|screenSize"
            android:hardwareAccelerated="true"
            android:label="MajVjExample3">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>
